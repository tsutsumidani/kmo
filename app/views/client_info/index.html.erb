    <div class="inner-header">
      <h4 class="page-header"><i class="fa fa-dashboard fa-fw"></i>会社情報</h4>
    </div>
  </div>
  <h5 class="page-header"><%= @client_info_form.client.client_nm %></h5>
  <%= render 'client_info/news_feed' %>
